generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model orders {
  id           String   @id @default(uuid())
  products     String
  total_amount String
  address      String
  user_id      String
  created_at   DateTime @default(now())
}
